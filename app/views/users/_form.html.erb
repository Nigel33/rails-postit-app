<div class="well">
	<%= form_for object do |f| %>
		<div class="control-group">
			<%= f.label :username %>
			<%= f.text_field :username %> 
		</div>
		<div class="control-group">
			<%= f.label :password %>
			<%= f.password_field :password %> 
		</div>
		<%= f.submit (object.new_record? ? "Register" : "Update"), class: 'btn btn-success' %>
	<% end %>
</div>